{
  "name": "geradordetags-producao",
  "version": "0.13.0",
  "description": "Gerador de Tags Exclusivas - Frontend",
  "main": "server.js",
  "config": {
    "commitMessage": "Automatizado o Deploy"
  },
  "scripts": {
    "dev": "http-server public -o",
    "start": "node server.js",
    "git": "npm run gitstatus && npm run gitadd && npm run gitcommit && npm run gittag && npm run gitpushorigin && npm run gitpushbitbucket",
    "gitstatus": "git status",
    "gitadd": "git add .",
    "gitcommit": "git commit -m \"%npm_package_config_commitMessage%\"",
    "gittag": "git tag v%npm_package_version%",
    "gitpushorigin": "git push origin master --tags",
    "gitpushbitbucket": "git push bitbucket master --tags",
    "deploy": "we deploy -p gte -v"
  },
  "engines": {
    "node": "8.9.0"
  },
  "keywords": [
    "gerador",
    "tags"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "pug": "^2.0.0-rc.4"
  }
}
