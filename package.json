{
  "name": "geradordetags-producao",
  "version": "0.19.0",
  "description": "Gerador de Tags Exclusivas - Frontend",
  "main": "server.js",
  "config": {
    "commitMessage": "Melhoria no controle de vers√£o automatizado"
  },
  "scripts": {
    "dev": "nodemon dev.js",
    "start": "node server.js",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpb",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpb": "git push bitbucket master --tags",
    "deploy": "we deploy -p gte -v",
    "standard": "standard"
  },
  "engines": {
    "node": "8.11.1"
  },
  "keywords": [
    "gerador",
    "tags"
  ],
  "author": "Gabriel Bertola Bocca - gabriel@estudiodigitalbocca.com.br",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "nodemon": "^1.17.3",
    "standard": "^11.0.1"
  }
}
